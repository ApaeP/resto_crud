<%= form_with(model: restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, placeholder: 'coucou' %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.submit %>
<% end %>
